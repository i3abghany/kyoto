// NAME IfComparison
// ERR 0
// RET -1

fn main() i32 {
    var a: i32 = 1;
    var b: i32 = 2;
    if (a < b) {
        return -1;
    } else if (a > b) {
        return 1;
    }
    return 0;
}


// NAME IfComparison2
// ERR 0
// RET 1

fn main() i32 {
    var a: i32 = 10;
    var b: i32 = 2;
    if (a > b) {
        return 1;
    } else if (a < b) {
        return -1;
    } else {
        return 0;
    }
}