// NAME IfGreaterThan
// ERR 0
// RET 1

fn main() i32 {
    if (1 > 0) {
        return 1;
    }
    return 0;
}


// NAME IfLessThan
// ERR 0
// RET 1

fn main() i32 {
    if (0 < 1) {
        return 1;
    }
    return 0;
}


// NAME IfEquals
// ERR 0
// RET 1

fn main() i32 {
